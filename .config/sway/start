# set WAYLAND_DISPLAY/DISPLAY in systemd user environment, see: FS#63021
exec systemctl --user import-environment WAYLAND_DISPLAY DISPLAY
exec hash dbus-update-activation-environment 2>/dev/null \
  && dbus-update-activation-environment --systemd WAYLAND_DISPLAY DISPLAY

exec $app_idle
exec mako
exec $bin/wlsunset-with-config

